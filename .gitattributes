*.hbs text
*.dot text
*.tmpl text
*.phtml text
*.latte text

# server config
.htaccess text

# git config
.gitattributes text
.gitignore text
.gitconfig text

# code analysis config
.jshintrc text
.jscsrc text
.jshintignore text
.csslintrc text

# misc config
*.yaml text
*.yml text
.editorconfig text

# build config
*.npmignore text
*.bowerrc text

# Documents
*.md text
LICENSE text
AUTHORS text
